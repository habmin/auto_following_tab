{

  "manifest_version": 2,
  "name": "Auto-Following Tab",
  "version": "1.0",

  "description": "Defaults to the 'Following' tab on twitter/home",

  "icons": {
    "48": "icons/border-48.png"
  },

  "content_scripts": [
    {
      "matches": ["*://*.twitter.com/home"],
      "js": ["auto_following_tab.js"]
    }
  ]

}
